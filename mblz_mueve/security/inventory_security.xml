        <record id="stock.group_stock_user" model="res.groups">
            <field name="active" eval="False"/>
            <field name="implied_ids" eval="[(6, 0, [])]"/>
        </record>

        <record id="stock.group_stock_manager" model="res.groups">
            <field name="implied_ids" eval="[(6, 0, [])]"/>
        </record>

        <record id="mueve_group_stock_analista_inventario" model="res.groups">
            <field name="name">Analista de Almacén (mueve)</field>
            <field name="category_id" ref="base.module_category_inventory_inventory"/>
            <field name="comment">Una copia del usuario administrador con las restricciones para mueve</field>
        </record>

        <record id="base.default_user" model="res.users">
            <field name="groups_id" eval="[(4,ref('mueve_group_stock_analista_inventario'))]"/>
        </record>


        <record id="mueve_group_stock_auxillar_almacen" model="res.groups">
            <field name="name">Auxiliar del almacén (mueve)</field>
            <field name="category_id" ref="base.module_category_inventory_inventory"/>
        </record>


