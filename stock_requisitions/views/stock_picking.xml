<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_stock_picking_form" model="ir.ui.view">
        <field name="name">stock.picking.form</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form" />
        <field name="arch" type="xml">
            <field name="origin" position="after">
                <field name="is_requisition" invisible="1"/>
            </field>

            <xpath expr="//field[@name='move_ids_without_package']//field[@name='quantity_done']" position="after">
                <field name="to_return" attrs="{'column_invisible': [('parent.is_requisition','=',False)]}"/>
                <field name="to_return_date" attrs="{'column_invisible': [('parent.is_requisition','=',False)]}"/>
            </xpath>
        </field>
    </record>

    <record id="view_stock_move_line_detailed_operation_tree" model="ir.ui.view">
        <field name="name">stock.move.line.operations.tree</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_stock_move_line_detailed_operation_tree" />
        <field name="arch" type="xml">
            <field name="qty_done" position="after">
                <field name="to_return" attrs="{'column_invisible': [('parent.is_requisition','=',False)]}"/>
                <field name="to_return_date" attrs="{'column_invisible': [('parent.is_requisition','=',False)]}"/>
            </field>
        </field>
    </record>
</odoo>